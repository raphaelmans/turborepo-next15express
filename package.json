{
  "name": "with-docker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build --filter='!web-old'",
    "clean": "turbo run clean --filter='!web-old'",
    "dev": "turbo run dev --filter='!web-old'",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "prepare": "husky"
  },
  "devDependencies": {
    "husky": "^9.1.6",
    "prettier": "^3.2.5",
    "turbo": "^2.2.3"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
